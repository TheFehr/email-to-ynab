{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "$id": "https://github.com/thefehr/email-to-ynab/lib/helpers/config/schema.json",
  "type": "object",
  "properties": {
    "transfer_ids": {
      "type": "object",
      "properties": {
        "type": {
          "type": "string",
          "pattern": "([A-Z0-9]{8}-[A-Z0-9]{4}-[A-Z0-9]{4}-[A-Z0-9]{4}-[A-Z0-9]{12})"
        }
      }
    },
    "email_parts_regexps": {
      "type": "object",
      "properties": {
        "entry_type": {
          "type": "string"
        },
        "account": {
          "type": "string"
        },
        "amount": {
          "type": "string"
        },
        "booking_date": {
          "type": "string"
        },
        "valuta_date": {
          "type": "string"
        },
        "memo": {
          "type": "string"
        }
      },
      "additionalProperties": false
    },
    "payee_name_regexps": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "additionalProperties": false
}